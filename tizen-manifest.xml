<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="2.4" package="org.example.iotivityexampleserver" version="1.0.0">
    <author email="philippe.coval@osg.samsung.com" href="https://wiki.tizen.org/wiki/User:Pcoval">Philippe Coval</author>
    <description>IoTivity Geolocation example</description>
    <profile name="mobile"/>
    <ui-application appid="org.example.iotivityexampleserver" exec="iotivityexampleserver" launch_mode="single" multiple="false" nodisplay="false" taskmanage="true" type="capp">
        <label>IoTivity Geolocation server</label>
        <icon>logo.png</icon>
    </ui-application>
    <privileges>
      <privilege>http://tizen.org/privilege/internet</privilege>
      <privilege>http://tizen.org/privilege/location</privilege>
    </privileges>
    <feature name="http://tizen.org/feature/location.gps">true</feature>
    <feature name="http://tizen.org/feature/screen.size.all">true</feature>
</manifest>
